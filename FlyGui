plus.TextColor3 = Color3.new(1, 1, 1)
local plusCorner = Instance.new("UICorner", plus)
plusCorner.CornerRadius = UDim.new(0, 8)

-- SPEED (TextBox) dengan properti visual mirip TextLabel
speed.Name = "SpeedInput" -- Mengganti nama dari SpeedLabel
speed.Parent = Frame
speed.Size = UDim2.new(0, 60, 0, 35)
speed.Position = UDim2.new(plus.Position.X.Scale + (plus.Size.X.Offset / Frame.AbsoluteSize.X) + 0.02, 0, 0, 40)
speed.AnchorPoint = Vector2.new(0, 0)
speed.BackgroundColor3 = Color3.fromRGB(30, 30, 30) -- Warna latar belakang sama
speed.Text = "1"
speed.Font = Enum.Font.GothamBold
