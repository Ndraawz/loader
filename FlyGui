onof.Parent = Frame
onof.Size = UDim2.new(0.8, 0, 0, 45)
onof.Position = UDim2.new(0.5, 0, 0, 140)
onof.AnchorPoint = Vector2.new(0.5, 0)
onof.BackgroundColor3 = Color3.fromRGB(180, 0, 0)
onof.Text = "FLY: OFF"
onof.Font = Enum.Font.GothamBold
onof.TextSize = 22
onof.TextColor3 = Color3.new(1, 1, 1)
local onofCorner = Instance.new("UICorner", onof)
onofCorner.CornerRadius = UDim.new(0, 8)

-- Penempatan untuk PLUS
plus.Name = "PlusButton"
plus.Parent = Frame
plus.Size = UDim2.new(0, 40, 0, 35)
plus.Position = UDim2.new(0.05 + 0.45 + 0.02, 0, 0, 40)
plus.AnchorPoint = Vector2.new(0, 0)
plus.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
plus.Text = "+"
plus.Font = Enum.Font.GothamBold
