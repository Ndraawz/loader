speed.ClearTextOnFocus = false -- Penting: agar teks tidak hilang saat diklik
speed.ClipsDescendants = true -- Membantu dalam beberapa kasus
speed.TextWrapped = false
speed.MultiLine = false
speed.PlaceholderText = "" -- Pastikan tidak ada placeholder yang mengganggu
speed.BorderSizePixel = 0 -- Menghilangkan border khas TextBox
local speedCorner = Instance.new("UICorner", speed)
speedCorner.CornerRadius = UDim.new(0, 8)

-- Penempatan untuk MINUS
mine.Name = "MinusButton"
